function smvcApplyFilter(filterList) {
    pattern = '';
    for (var i = 0; i < filterList.length; i++) {
        if (filterList[i] !== "") {
            pattern += filterList[i] + '.+';
        }
    }
    var match = [];
    var results = [];
    for(var i=0; i < renderList.length; i++) {
        match = renderList[i].match('/' + pattern +'/');
        if (match != null)
            results.push(renderList[i]);
    }
    return results;
}

